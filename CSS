<script type="text/javascript">// <![CDATA[
function Calculate()
{
        num1 = document.getElementById("WattsPerLamp").value;
        num2 = document.getElementById("PpfPerLamp").value;
        num3 = document.getElementById("NumberOfLamps").value;
        num4 = document.getElementById("CostPerLamp").value;
        num5 = document.getElementById("EnergyRate").value;
        num6 = document.getElementById("LightsOnHrs").value;
        num7 = document.getElementById("CycleDays").value;

var CalcualtedEfficiency= (num2 / num1 );
document.getElementById("CalcualtedEfficiency").innerHTML = CalcualtedEfficiency.toFixed(2);

var TotalPPF= (num2 * num3);
document.getElementById("TotalPPF").innerHTML = TotalPPF.toFixed(2);

var TotalFixtureCost= (num3 * num4);
document.getElementById("TotalFixtureCost").innerHTML = TotalFixtureCost.toFixed(2);

var TotalPowerConsum= (num3 * num1 );
document.getElementById("TotalPowerConsum").innerHTML = TotalPowerConsum.toFixed(2);

var TotalPowerConsumKW= (num3 * num1 /1000 );
document.getElementById("TotalPowerConsumKW").innerHTML = TotalPowerConsumKW.toFixed(2);

var TotalBTU= (num3 * num1 * 3.412141633 );
document.getElementById("TotalBTU").innerHTML = TotalBTU.toFixed(2);

var TotalEnergyperday= ( num6 * num5 * num3 * num1 /1000 );
document.getElementById("TotalEnergyperday").innerHTML = TotalEnergyperday.toFixed(2);

var TotalEnergypercycle= ( num7 * num6 * num5 * num3 * num1 /1000 );
document.getElementById("TotalEnergypercycle").innerHTML = TotalEnergypercycle.toFixed(2);

}
// ]]></script>
